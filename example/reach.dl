.decl Source(id: number)
.input Source(IO="file", filename="livejournal/Source.csv", delimiter=",")
.decl Arc(x: number, y: number)
.input Arc(IO="file", filename="livejournal/Arc.csv", delimiter=",")

.decl Reach(id: number)

Reach(y) :- Source(y).
Reach(y) :- Reach(x), Arc(x,y).

.printsize Reach
