.in 
.decl arc(src: integer, dest: integer, weight: integer) 
.input Arc.csv
.decl id(src: integer)
.input id.csv

.out 
.decl sssp2(x: integer, y: integer)
.decl sssp(x: integer, y: integer)

.rule
sssp2(x, min(0)) :- id(x).
sssp2(y, min(d1 + d2)) :- sssp2(x, d1), arc(x, y, d2).
sssp(x, min(d)) :- sssp2(x, d).